<html>
<head>
<title>Run Batch File</title>
<HTA:APPLICATION
    APPLICATIONNAME="Run Batch File"
    SCROLL="no"
    SINGLEINSTANCE="yes"
    WINDOWSTATE="normal"
>
</head>
<body>
<input type="button" value="Run Batch File" onclick="runBatchFile()">
<div id="output"></div>
<script language="VBScript">
	Sub runBatchFile
		Set WshShell = CreateObject("WScript.Shell")
		Set oExec = WshShell.Exec("cmd /c N:\bat\HTA\1.bat")
		strOutput = oExec.StdOut.ReadAll
		strOutput = Replace(strOutput, vbCrLf, "<br>")
		document.getElementById("output").innerHTML = strOutput
	End Sub
</script>
</body>
</html>